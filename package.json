{
  "name": "juritj",
  "version": "0.0.1",
  "description": "JURITJ",
  "author": "Cour de Cassation",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" --check",
    "start": "NODE_ENV=local nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/api/main",
    "start:docker": "docker compose up -d",
    "stop:docker": "docker compose stop",
    "batch:start": "npx ts-node -r dotenv/config src/batch/normalization/index.ts",
    "batch:start:watch": "nodemon --config nodemon-batch.json",
    "batch:start:prod": "node -r dotenv/config dist/batch/normalization/index.js",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "test": "jest && npm run test:integration",
    "test:batch": "jest --testPathPattern=src/batch",
    "test:api": "jest --testPathPattern=src/api & npm run test:integration",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./jest-integration.json --runInBand",
    "fix": "eslint \"src/**/*.ts\" --fix && prettier \"src/**/*.ts\" --write"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.888.0",
    "@nestjs/axios": "3.1.3",
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.20",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/terminus": "10.3.0",
    "axios": "1.12.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cron": "2.4.4",
    "dbsder-api-types": "2.1.9",
    "express-basic-auth": "1.2.1",
    "joi": "17.13.3",
    "nestjs-pino": "3.5.0",
    "pino-http": "8.6.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.10",
    "rxjs": "7.8.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.20",
    "@smithy/util-stream": "2.2.0",
    "@types/express": "4.17.23",
    "@types/jest": "29.5.14",
    "@types/multer": "1.4.13",
    "@types/node": "20.19.15",
    "@types/supertest": "2.0.16",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "aws-sdk-client-mock": "3.1.0",
    "aws-sdk-client-mock-jest": "3.1.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "nodemon": "3.1.10",
    "pino-pretty": "10.3.1",
    "prettier": "3.6.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.4.2",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/.jest/setupEnvVars.ts"
    ]
  }
}