{
  "name": "juritj",
  "version": "0.0.1",
  "description": "JURITJ",
  "author": "Cour de Cassation",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" --check",
    "start": "NODE_ENV=local nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/api/main",
    "docker:build": "docker compose -f docker-compose.local.yml build",
    "docker:start": "docker compose -f docker-compose.local.yml up -d",
    "docker:logs": "docker compose -f docker-compose.local.yml logs --follow",
    "docker:stop": "docker compose -f docker-compose.local.yml stop",
    "docker:kill": "docker compose -f docker-compose.local.yml down --remove-orphans --volumes --rmi local",
    "batch:start": "npx ts-node -r dotenv/config src/batch/normalization/index.ts",
    "batch:start:watch": "nodemon --config nodemon-batch.json",
    "batch:start:prod": "node -r dotenv/config dist/batch/normalization/index.js",
    "docker:start:s3": "docker compose -f docker-compose.local.yml up -d bucket && docker compose -f docker-compose.local.yml up -d createbuckets",
    "docker:stop:s3": "docker compose -f docker-compose.local.yml stop bucket",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet",
    "test": "jest && npm run test:integration",
    "test:batch": "jest --testPathPattern=src/batch",
    "test:api": "jest --testPathPattern=src/api & npm run test:integration",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./jest-integration.json --runInBand",
    "fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && prettier \"src/**/*.ts\" --write"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.441.0",
    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "10.2.8",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.8",
    "@nestjs/platform-express": "10.2.8",
    "@nestjs/swagger": "7.1.14",
    "@nestjs/terminus": "10.1.1",
    "axios": "1.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cron": "2.4.3",
    "dbsder-api-types": "1.4.4",
    "express-basic-auth": "1.2.1",
    "joi": "17.11.0",
    "nestjs-pino": "3.5.0",
    "pino-http": "8.5.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.7",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.8",
    "@smithy/util-stream": "2.0.17",
    "@types/express": "4.17.20",
    "@types/jest": "29.5.7",
    "@types/multer": "1.4.9",
    "@types/node": "20.8.10",
    "@types/supertest": "2.0.15",
    "@types/uuid": "9.0.6",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "aws-sdk-client-mock": "3.0.0",
    "aws-sdk-client-mock-jest": "3.0.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "nodemon": "^3.1.4",
    "pino-pretty": "10.2.3",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/.jest/setupEnvVars.ts"
    ]
  }
}
